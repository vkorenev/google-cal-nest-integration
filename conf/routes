GET         /                           controllers.MainController.selectStructure
GET         /receiveNestAuthCode        controllers.MainController.receiveNestAuthCode(code)
GET         /setStructure               controllers.MainController.setStructure(structureId)

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
